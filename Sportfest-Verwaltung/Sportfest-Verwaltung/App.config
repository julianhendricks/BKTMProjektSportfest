<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <startup> 
      <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <system.data>
    <DbProviderFactories>
      <remove invariant="MySql.Data.MySqlClient" />
      <add name="MySQL Data Provider" invariant="MySql.Data.MySqlClient" description=".Net Framework Data Provider for MySQL" type="MySql.Data.MySqlClient.MySqlClientFactory, MySql.Data, Version=6.9.7.0, Culture=neutral, PublicKeyToken=c5687fc88969c44d" />
    </DbProviderFactories>
  </system.data>
  <appSettings>
    <add key="mySql_host" value="localhost" />
    <add key="mySql_username" value="root" />
    <add key="mySql_password" value="" />
    <add key="mySql_port" value="3306" />
    <add key="mySql_database" value="sportsFestival1" />
    <add key="enable_sql_logger" value="true" />
  </appSettings>
</configuration>